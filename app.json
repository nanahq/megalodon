{
  "expo": {
    "name": "EatLater",
    "description": "Order food from your favorite vendors and restaurants",
    "slug": "wallet",
    "owner": "nanahq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-config/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/app-config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-config/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/app-config/favicon.png"
    }
  }
}
