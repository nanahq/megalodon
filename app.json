{
  "expo": {
    "name": "Nana",
    "description": "Order food online and get it delivered in minutes",
    "slug": "nana-main",
    "owner": "nana_logistics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-config/Icon-blue-ios180.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsImpl": "mapbox",
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic3VyYWphdXdhbCIsImEiOiJjbHAwY25wcjEwM2Z1MnFwamc2bDd6Mmx0In0.mvt4AIl-lQDehhIFIdfSrQ"
        }
      ]
    ],
    "splash": {
      "image": "./assets/app-config/nana-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00C2E8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nana-app.nana-main"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-config/Icon-Android-512px.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.nanaapp.nanamain"
    },
    "web": {
      "favicon": "./assets/app-config/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4cb797ba-86fc-4789-9d3a-fda5e16fd634"
      }
    }
  }
}
